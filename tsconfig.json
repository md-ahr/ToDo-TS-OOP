{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["ES2018", "DOM"],
      "allowJs": true,
      "sourceMap": true,
      "removeComments": true,
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": false,
      "strictFunctionTypes": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "esModuleInterop": true,
      "moduleResolution": "node"
    }
}